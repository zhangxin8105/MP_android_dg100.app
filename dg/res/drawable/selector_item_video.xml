<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/videoSelect_radian_recycle_item" />
            <solid android:color="@color/videoPlayer_font_recycle_item_selected" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape>
            <corners android:radius="@dimen/videoSelect_radian_recycle_item" />
            <solid android:color="@color/videoPlayer_font_recycle_item_selected" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape>
            <corners android:radius="@dimen/videoSelect_radian_recycle_item" />
            <solid android:color="@color/videoPlayer_font_recycle_background_unselected" />
        </shape>
    </item>
</selector>